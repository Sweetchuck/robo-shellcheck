{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-shellcheck",
    "description": "@todo project description",
    "keywords": [
        "robo-tasks",
        "shellcheck"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/robo-shellcheck",
    "support": {
        "source": "https://github.com/Sweetchuck/robo-shellcheck",
        "issues": "https://github.com/Sweetchuck/robo-shellcheck/issues"
    },
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "repositories": {},
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "consolidation/robo": "^2.0",
        "sweetchuck/lint-report": "1.x-dev",
        "sweetchuck/utils": "1.x-dev"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "nuvoleweb/robo-config": "2.x-dev",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6",
        "sweetchuck/codeception-module-robo-task-runner": "1.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "1.x-dev",
        "sweetchuck/robo-phpcs": "1.x-dev",
        "sweetchuck/robo-phpmd": "^1.x-dev",
        "symfony/error-handler": "^5.0",
        "symfony/yaml": "^4.0 || ^5.0",
        "webmozart/path-util": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\Shellcheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\Shellcheck\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\Shellcheck\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\Shellcheck\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {},
    "extra": {}
}
